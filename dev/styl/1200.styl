    &:hover
        background-image: url(../img/colored/ic_soc-footer_hover.png);


.floormenu
    text-align: center;
    padding: 18px 0 12px;
    border-bottom: 1px solid #c9ddea;
    transition: margin .2s, padding .2s;
    &_wrapper
        &.active
            height: 50px;
    &.isStuck
        background: rgba(255, 255, 255, 0.9);
