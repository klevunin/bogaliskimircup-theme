            .bigfilter__item
                cursor: default;
                &:hover
                    .bigfilter__link
                        color: #666666;
        &_oneline
            .bigfilter__item
                cursor: pointer;
        &_flags
            position: relative;
            &:before
                position: absolute;
                display: block;
                content: "";
                background: url(../img/bg/bg_flag_l.png);
                width: 278px;
                height: 235px;
                left: -278px;
                top: 85px;
            &:after
